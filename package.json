    "helmet": "^3.21.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.21.1",
    "knex-postgis": "^0.12.0",
    "morgan": "^1.9.1",
    "pg": "^8.1.0",
    "postgrator": "^4.0.0",
    "postgrator-cli": "^3.3.0",
    "socket.io": "^2.4.1",
    "uuid": "^8.1.0",
    "uuidv4": "^6.0.9",
